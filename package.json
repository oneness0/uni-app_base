{
  "scripts": {
    "lint:eslint": "eslint --fix",
    "lint:stylelint": "stylelint --fix",
    "lint:markdownlint": "markdownlint --ignore node_modules",
    "lint:prettier": "prettier --write --loglevel warn",
    "lint": "run-s lint:all:*",
    "lint:all:eslint": "yarn lint:eslint --ext .js,.vue .",
    "lint:all:stylelint": "yarn lint:stylelint \"pages/**/*.{vue,scss}\"",
    "lint:all:markdownlint": "yarn lint:markdownlint \"**/*.md\"",
    "lint:all:prettier": "yarn lint:prettier \"**/*.{js,json,css,scss,vue,html,md}\"",
    "tem": "node scripts/template",
    "com": "node scripts/component",
    "uni": "dgit dcloudio/uni-ui/src/components/ -r master",
    "postinstall": ""
  },
  "dependencies": {
    "dgit": "^0.0.6",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-standard": "^5.1.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.1.2",
    "husky": "^4.0.10",
    "lint-staged": "^9.5.0",
    "markdownlint-cli": "^0.21.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "stylelint": "^13.0.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recess-order": "^2.0.3",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-scss": "^3.13.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "cross-env PRE_COMMIT=true lint-staged"
    }
  },
  "license": "MIT"
}
